{
  "name": "dtstorage",
  "version": "1.1.1",
  "description": "ES6 class for REST-like management local- & sessionStorage",
  "main": "dist/storage.min.js",
  "scripts": {
    "build:browserify": "$(npm bin)/browserify -t babelify src/storage.js > dist/storage.js",
    "build:uglify": "$(npm bin)/uglifyjs dist/storage.js --output dist/storage.min.js --source-map dist/storage.min.js.map",
    "build": "npm run build:browserify && npm run build:uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtrussia/storage.js.git"
  },
  "keywords": [
    "storage",
    "sessionstorage",
    "localstorage"
  ],
  "author": "Eugene Rodionov (https://github.com/theaqua)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dtrussia/storage.js/issues"
  },
  "homepage": "https://github.com/dtrussia/storage.js#readme",
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.0",
    "uglify-js": "^2.4.24"
  }
}
